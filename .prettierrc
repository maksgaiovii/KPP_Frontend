{
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "bracketSpacing": true,
  "endOfLine": "lf",

  "importOrder": ["^react.*$", "<THIRD_PARTY_MODULES>", "^components/(.*)$", "^[./](.*)$", "^../(.*)$"]
}
